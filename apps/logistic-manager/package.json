{
  "name": "@acme/logistic-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "dotenv -e ../../.env -e .env.local -- bash -c 'next build'",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "dotenv -e ../../.env -e .env.local -- bash -c 'next dev -p \"$PORT\"'",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --ignore-path ../../.gitignore --check \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "dotenv -e ../../.env -e .env.local -- bash -c 'next start -p \"$PORT\"'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@acme/api": "^0.1.0",
    "@acme/auth": "^0.1.0",
    "@acme/db": "^0.1.0",
    "@acme/ui": "^0.1.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.0",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@tanstack/react-table": "^8.9.3",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "lodash.get": "^4.4.2",
    "nanoid": "^4.0.2",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "react": "18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-tailwindcss-datepicker": "^1.6.6",
    "superjson": "1.13.1",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2",
    "zod-form-data": "^2.0.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^18.17.7",
    "@types/react": "^18.2.21",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^18.2.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/nextjs",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
